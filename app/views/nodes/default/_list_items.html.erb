<%- item.children.non_badgeable.each do |child_node| -%>
	<%- if child_node.prop.is_a? PileRefProp -%>
		<%= render_subscoped :partial => 'nodes/sub_pile_item', :object => child_node, :subscope => subscope_of(child_node.prop.ref_pile) %>
	<%- else -%>
		<%= render_subscoped :partial => 'nodes/show_item', :locals => {:item => child_node} %>
	<%- end -%>
<%- end -%>
