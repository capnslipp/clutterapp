<div class="show body">
	<div class="bullet">
		<% if @check.present? %>
			<%= render_cell cell_for_node(@check), :show, :node => @check %>
		<% else %>
			<span class="sym">â€¢</span>
		<% end %>
	</div>
	<%= render :partial => 'nodes/action_stub' %>
	<div class="cont" onclick="void(0)"><%# onclick, for MobileSafari, with Love -%>
		<% if @priority.present? %>
			<%= render_cell cell_for_node(@priority), :show, :node => @priority %>
		<% end %>
		
		<%= render_cell(cell_for_node(@node), :show, :node => @node) %>
		
		<% if @time.present? %>
			<div class="line">
				<%= render_cell cell_for_node(@time), :show, :node => @time %>
			</div>
		<% end %>
		<% if @tags.present? %>
			<div class="line">
				<%- for tag in @tags -%>
					<%= render_cell cell_for_node(tag), :show, :node => tag %>
				<%- end unless @tags.nil? -%>
			</div>
		<% end %>
		<% if @note.present? %>
			<div class="line">
				<%= render_cell cell_for_node(@note), :show, :node => @note %>
			</div>
		<% end %>
	</div>
</div>
